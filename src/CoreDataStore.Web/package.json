{
  "name": "CoreDataStore",
  "version": "1.0.0",
  "description": "CoreDataStore Angular2 with TypeScript and Gulp",
  "private": true,
  "scripts": {
    "clean": "./node_modules/.bin/gulp clean",
    "compile": "./node_modules/.bin/gulp compile",
    "build": "./node_modules/.bin/gulp build",
    "ghpage": "./node_modules/.bin/gulp ghpage",
    "build:dev": "./node_modules/.bin/gulp build:dev",
    "postinstall": "./node_modules/.bin/typings install",
    "start": "concurrently --kill-others \"./node_modules/.bin/gulp watch\" \"./node_modules/.bin/lite-server --host=0.0.0.0\"",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:w": "./node_modules/.bin/tsc -w",
    "typings": "./node_modules/.bin/typings"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "dependencies": {
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/router": "^4.0.2",
    "@angular/upgrade": "^4.0.2",
    "ngx-infinite-scroll": "^0.5.0",
    "angular2-toaster": "^3.0.1",
    "core-js": "2.4.1",
    "es6-shim": "^0.35.0",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "simple-line-icons": "^2.4.1",
    "jquery": "^3.2.1",
    "jquery-slimscroll": "^1.3.8",
    "jquery.browser": "^0.1.0",
    "screenfull": "^3.2.0",
    "ngx-bootstrap": "1.6.6",
    "ng2-select": "^1.2.0",
    "ng2-table": "^1.3.2",
    "reflect-metadata": "^0.1.10",
    "systemjs": "^0.20.12",
    "rxjs": "5.3.0",
    "zone.js": "^0.8.8",
    "angular2-google-maps": "^0.16.0",
    "ng2-translate": "^4.2.0"
  },
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "concurrently": "^3.4.0",
    "connect-history-api-fallback": "^1.3.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-coveralls": "^0.1.4",
    "gulp-cssmin": "^0.1.7",
    "gulp-gh-pages": "^0.5.4",
    "gulp-if": "^2.0.2",
    "gulp-less": "^3.3.0",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-ts-config": "^1.0.15",
    "gulp-tslint": "^8.0.0",
    "gulp-typescript": "^3.1.6",
    "gulp-uglify": "^2.1.2",
    "http-proxy-middleware": "^0.17.4",
    "lite-server": "^2.3.0",
    "merge-stream": "^1.0.0",
    "systemjs-builder": "^0.16.4",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "typings": "^2.1.1"
  }
}
